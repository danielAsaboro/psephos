{
  "name": "psephos",
  "description": "Private voting system using Noir ZK proofs on Solana",
  "keywords": [
    "anchor",
    "noir",
    "zero-knowledge",
    "privacy",
    "voting",
    "governance",
    "solana",
    "hackathon",
    "react",
    "typescript",
    "vite"
  ],
  "create-solana-dapp": {
    "rename": {
      "scaffold": {
        "to": "{{name}}",
        "paths": [
          "src"
        ]
      }
    }
  },
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "anchor-build": "cd anchor && anchor build",
    "anchor-test": "cd anchor && anchor test",
    "build": "tsc -b && vite build",
    "build:circuits": "cd circuits && nargo compile",
    "prove:circuits": "cd circuits && nargo prove",
    "verify:circuits": "cd circuits && nargo verify",
    "check:circuits": "cd circuits && nargo check",
    "ci": "npm run build && npm run lint && npm run format:check",
    "codama:js": "codama run js",
    "dev": "vite",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "preview": "vite preview",
    "setup": "npm run anchor-build && npm run codama:js"
  },
  "dependencies": {
    "@noir-lang/backend_barretenberg": "^0.36.0",
    "@noir-lang/noir_js": "^1.0.0-beta.18",
    "@solana/client": "^1.2.0",
    "@solana/kit": "^5.1.0",
    "@solana/react-hooks": "^1.1.5",
    "@tailwindcss/vite": "^4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4"
  },
  "devDependencies": {
    "@codama/nodes-from-anchor": "^1.3.8",
    "@codama/renderers-js": "^1.5.5",
    "@eslint/js": "^9.35.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.3",
    "codama": "^1.5.0",
    "eslint": "^9",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "terser": "^5.46.0",
    "typescript": "^5",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.6"
  }
}
